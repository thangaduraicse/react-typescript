    "@types/redux-thunk": "^2.1.31",
    "@types/underscore": "^1.7.34",
    "@types/whatwg-fetch": "0.0.32",
    "autoprefixer": "^6.5.3",
    "awesome-typescript-loader": "2.2.4",
    "babel-core": "^6.18.2",
